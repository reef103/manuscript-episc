	mesh = {Antineoplastic Agents; Cell Line, Tumor; Gene Expression Profiling; Gene Expression Regulation, Neoplastic; Gene Regulatory Networks; Genome; Genome-Wide Association Study; Genomics; High-Throughput Nucleotide Sequencing; Humans; Reproducibility of Results},
	month = {07},
